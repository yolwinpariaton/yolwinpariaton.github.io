{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "CPIH annual inflation rate (UK)"
  },
  "data": {
    "url": "data/chart1_cpih_annual_rate.json"
  },
  "width": "container",
  "height": 300,
  "mark": {
    "type": "line",
    "point": true
  },
  "encoding": {
    "x": {
      "field": "date",
      "type": "temporal",
      "title": "Date"
    },
    "y": {
      "field": "value",
      "type": "quantitative",
      "title": "Percent"
    },
    "tooltip": [
      {
        "field": "date",
        "type": "temporal"
      },
      {
        "field": "value",
        "type": "quantitative"
      }
    ]
  }
}