{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Rent inflation by English region (latest month)"
  },
  "data": {
    "url": "data/chart7_rent_inflation_regions.json"
  },
  "width": "container",
  "height": 320,
  "mark": {
    "type": "bar"
  },
  "encoding": {
    "y": {
      "field": "region",
      "type": "nominal",
      "sort": "-x",
      "title": ""
    },
    "x": {
      "field": "rent_inflation_yoy_pct",
      "type": "quantitative",
      "title": "Annual rent inflation (%)"
    },
    "tooltip": [
      {
        "field": "region"
      },
      {
        "field": "rent_inflation_yoy_pct",
        "type": "quantitative"
      }
    ]
  }
}